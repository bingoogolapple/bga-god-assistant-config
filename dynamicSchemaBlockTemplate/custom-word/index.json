          }
        ],
        "fulfill": {
          "run": "const cacheKeyCustomWordCardData = \"customWordCardData\"\n\n// 展示页面内通知和系统通知\nconst sendNotification = (item) => {\n  $message.success(`${item.learningLanguage} ${item.fromLanguage}`)\n\n  // 创建系统通知\n  $chrome.notifications.create(\n    null,\n    {\n      type: \"basic\",\n      title: item.learningLanguage,\n      message: item.fromLanguage,\n      iconUrl:\n        \"https://d35aaqx5ub95lt.cloudfront.net/images/pathCharacters/guidebook/939ebad747027f1a9d365e15fda3a4e1.svg\",\n    },\n    $chrome.proxy((notificationId) => {\n      // 延迟 6 秒清除系统通知\n      setTimeout(() => {\n        $chrome.notifications.clear(notificationId)\n      }, 6000)\n    })\n  )\n}\n\n// 添加或修改自定义单词\n$self.addOrUpdateCustomWord = async (data) => {\n  const list = [...$self.value]\n\n  if (data.id) {\n    // 修改\n    const index = list.findIndex((item) => item.id === data.id)\n    list[index] = data\n  } else {\n    // 添加\n    data.id = Date.now()\n    list.push(data)\n  }\n\n  try {\n    await $chrome.localStorage.setItem(\n      cacheKeyCustomWordCardData,\n      JSON.stringify(list)\n    )\n\n    $self.refreshCustomWordTable()\n  } catch (e) {\n    $logger.info(`${data.id ? \"修改\" : \"添加\"}自定义单词数据失败`, e)\n  }\n}\n\n// 获取自定义单词列表\n$self.fetchCustomWordList = async () => {\n  let res = null\n  try {\n    res = await $chrome.localStorage.getItem(cacheKeyCustomWordCardData)\n  } catch (e) {\n    $logger.info(\"获取自定义单词列表失败\", e)\n    return []\n  }\n\n  if (!res) {\n    return []\n  }\n\n  try {\n    const customWordCardData = JSON.parse(res) || []\n    return customWordCardData\n  } catch (e) {\n    $logger.info(\"解析自定义单词列表数据失败\", e)\n    return []\n  }\n}\n\n// 保存自定义单词列表数据\n$self.saveCustomWordList = async () => {\n  const list = $self.value\n  try {\n    await $chrome.localStorage.setItem(\n      cacheKeyCustomWordCardData,\n      JSON.stringify(list)\n    )\n  } catch (e) {\n    $logger.info(`保存自定义单词列表数据失败`, e)\n  }\n}\n\n// 刷新自定义单词列表\n$self.refreshCustomWordTable = async () => {\n  const customWordCardData = await $self.fetchCustomWordList()\n  $self.value = customWordCardData\n\n  if (customWordCardData.length > 0) {\n    const randomIndex = parseInt(Math.random() * customWordCardData.length)\n    const randomItem = customWordCardData[randomIndex]\n    sendNotification(randomItem)\n  }\n}\n\n$self.refreshCustomWordTable()\n"
        }
      },
